    "clean": "rimraf ./dist/*",
    "lint": "eslint 'src/**/*.ts'",
    "set-plugin-version": "ts-node build/scripts/set-plugin-version.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/runtime": "^7.16.7",
    "@rweich/streamdeck-ts": "^2.0.0",
    "@amille/semantic-release-plugins": "^3.3.10",
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@fnando/codestyle": "^0.0.13",
    "@fnando/eslint-config-codestyle": "^0.0.10",
    "@rweich/eslint-config": "^1.1.0",
    "@rweich/prettier-config": "^1.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/jsonfile": "^6.0.0",
    "babel-loader": "^8.2.2",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "copy-webpack-plugin": "^9.0.0",
    "eslint": "^8.8.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "jsonfile": "^6.1.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.3",
    "source-map-loader": "^3.0.0",
    "ts-node": "^10.0.0",
    "ts-type-guards": "^0.7.0",
    "typescript": "^4.5.5",
    "update-template": "^1.3.1",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
