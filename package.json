{
  "name": "@fnando/streamdeck-youtube-tools",
  "version": "0.0.0-development",
  "description": "Stream Deck plugin that displays Youtube's livestream elapsed time.",
  "private": true,
  "author": "Nando Vieira",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && webpack --mode=development",
    "build-prod": "yarn clean && webpack --mode=production",
